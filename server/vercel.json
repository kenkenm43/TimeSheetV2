{
  "version": 2,
  "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "https://time-sheet-v2.vercel.app",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Credentials": true
      }
    }
  ]
}
